    "@grafana/runtime": "^10.2.2",
    "@grafana/schema": "^10.2.2",
    "@grafana/ui": "^10.2.2",
    "@types/highlight.js": "^10.1.0",
    "@types/markdown-it": "^12.2.3",
    "@volkovlabs/components": "^1.2.1",
    "dayjs": "^1.11.10",
    "handlebars": "^4.7.8",
    "helper-date": "^1.0.1",
    "highlight.js": "^11.9.0",
    "markdown-it": "^13.0.2",
    "react": "^18.2.0",
    "react-beautiful-dnd": "^13.1.1",
    "react-dom": "^18.2.0",
    "tslib": "^2.6.2",
    "uuid": "^9.0.1"
  },
  "description": "A panel plugin for Grafana for dynamic, data-driven text",
  "devDependencies": {
    "@babel/core": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@babel/register": "^7.22.15",
    "@grafana/e2e": "^10.2.2",
    "@grafana/e2e-selectors": "^10.2.2",
    "@grafana/eslint-config": "^6.0.1",
    "@grafana/tsconfig": "^1.3.0-rc1",
    "@swc/core": "^1.3.96",
    "@swc/helpers": "^0.5.3",
    "@swc/jest": "^0.2.29",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@types/jest": "^29.5.8",
    "@types/lodash": "^4.14.201",
    "@types/node": "^18.18.10",
    "@types/react-beautiful-dnd": "^13.1.7",
    "@types/uuid": "^9.0.7",
    "@volkovlabs/eslint-config": "^1.2.2",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.8.1",
    "eslint-webpack-plugin": "^4.0.1",
    "fork-ts-checker-webpack-plugin": "^9.0.2",
    "glob": "^10.3.10",
    "identity-obj-proxy": "3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.1.1",
    "replace-in-file-webpack-plugin": "^1.0.6",
    "sass": "^1.69.5",
    "sass-loader": "^13.3.2",
    "style-loader": "^3.3.3",
    "swc-loader": "^0.2.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "5.3.2",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-livereload-plugin": "^3.0.2"
  },
  "engines": {
    "node": ">=18"
  },
  "license": "Apache-2.0",
  "name": "marcusolsson-dynamictext-panel",
  "scripts": {
    "build": "webpack -c ./webpack.config.ts --env production",
    "dev": "webpack -w -c ./webpack.config.ts --env development",
    "e2e": "npm exec cypress install && npm exec grafana-e2e run",
    "e2e:update": "npm exec cypress install && npm exec grafana-e2e run --update-screenshots",
    "levitate": "npx @grafana/levitate@latest is-compatible --path src/module.ts --target @grafana/data,@grafana/runtime,@grafana/ui",
    "lint": "eslint --ignore-path ./.eslintignore --ext .js,.jsx,.ts,.tsx .",
    "lint:fix": "eslint --fix --ignore-path ./.eslintignore --ext .js,.jsx,.ts,.tsx .",
    "sign": "npx --yes @grafana/sign-plugin@latest",
    "start": "docker-compose pull && docker-compose up",
    "stop": "docker-compose down",
    "test": "jest --watch --onlyChanged",
    "test:ci": "jest --maxWorkers 4 --coverage",
    "upgrade": "npm upgrade --save"
  },
  "version": "4.4.0"
}
