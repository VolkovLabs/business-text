    "@babel/core": "^7.24.0",
    "@babel/preset-typescript": "^7.23.3",
    "@babel/register": "^7.23.7",
    "@grafana/eslint-config": "^7.0.0",
    "@grafana/plugin-e2e": "^1.1.1",
