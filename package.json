    "@testing-library/react": "^14.2.1",
    "@types/jest": "^29.5.12",
    "@types/lodash": "^4.17.0",
    "@types/node": "^20.11.27",
    "@types/react-beautiful-dnd": "^13.1.8",
    "@types/uuid": "^9.0.8",
